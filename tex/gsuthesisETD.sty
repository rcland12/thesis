   \thispagestyle{myheadings}
   \vspace*{-25pt}
   {\normalsize \centering
   {\@chapapp{} \thechapter}
   {\\ {\uppercase{#1}} }  
   \\  [1\baselineskip] \vskip -20pt
   \nobreak}}
