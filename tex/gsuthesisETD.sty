\def\@dottedtocline#1#2#3#4#5{
    \ifnum #1>\c@tocdepth
    \else
    \vskip\baselineskip
       {\leftskip #2\relax \rightskip \@tocrmarg \parfillskip -\rightskip \nohyphen
	  \parindent #2\relax \@afterindenttrue \interlinepenalty \@M
	  \leavevmode \@tempdima #3\relax \advance \leftskip \@tempdima
	  \hbox {}\hskip -\leftskip #4 \nobreak
	  \leaders \hbox {$\m@th \mkern \@dotsep mu.\mkern \@dotsep mu$}\hfill
	  \nobreak \hbox to \@pnumwidth {\hfil \rm #5}\par}\fi}

