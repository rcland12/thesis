%%%%%%%%%%%%% Abstract Page %%%%%%%%%%%%%%%%%%

\renewenvironment{abstract}{%
  \doublespace
  \thispagestyle{empty}
  \begin{center}
    \normalsize{\uppercase\expandafter{\@title}} \\
    by \\ 
    {\uppercase\expandafter{\@author}} \\
    (Under the Direction of {\@committeechair}) \\
    \normalsize{ABSTRACT} \\
  \end{center}
  }
{
  \singlespace
  \vskip 50pt
  { \flushleft INDEX WORDS:  \expandafter{\@keywords} \\}
  \vskip 5pt
  { \flushleft 2009 Mathematics Subject Classification:  \expandafter{\@msc} \\}
  \vfill\newpage
}

%%%%%%%%%%%%%%% Title Page %%%%%%%%%%%%%%%%%%%%%
\newenvironment{titlep}{%
  \doublespace
  \thispagestyle{empty}
  \begin{center}
    \normalsize{\uppercase \expandafter{\@title} } \\
    by \\ 
    {\uppercase\expandafter{\@author}} \\
    { \normalsize
       \@degreeA

        \@degreeB

        \@degreeC

        \@degreeD
    }
    \noindent
    {\mbox{A {\@thesistype} Submitted to the Graduate Faculty of {\@school} in Partial} \\
    Fulfillment of the Requirements for the Degree} \\ 
    {\uppercase\expandafter{\@degree}} \\
     %{\@graduateyear}
  \end{center}
  \vfill\newpage
  }
{
}

%%%%%%%%%%%%%%%%%% Copyright Page %%%%%%%%%%%%%%%%%%%%
\newenvironment{copyrightpage}{%
\thispagestyle{empty}
    \doublespace
    \ \vfill
    \begin{center}
      \copyright {\@graduateyear} \\
      {\uppercase\expandafter{\@author}} \\
       All Rights Reserved
    \end{center}
    \newpage
   }

%%%%%%%%%%%%%%% Approval Page %%%%%%%%%%%%%%%%%%%%%
\newenvironment{approvalpage}{%
  \pagestyle{headings}
  \doublespace
  \setcounter{page}{1}
  \begin{center}
    \normalsize{\uppercase\expandafter{\@title}} \\
    by \\ 
    {\uppercase\expandafter{\@author}} \\
  \vfill
  \singlespace
  \begin{tabular}{ll}
    Major Professor: & {\@committeechair}  \\
    \ifnum\cochair>0 \phantom{Major Professor:} & {\@committeecochair} \\ \fi
    Committee: &{\@memberB}\\&{\@memberC}\\&{\@memberD}\\&{\@memberE}\\&{\@memberF}\\
  \end{tabular}
\end{center}
  \singlespace
  {\flushleft Electronic Version Approved: \\ {\@graduatedate} }
  \newpage
 }
{
}

%%%%%%%%%%%%%%% Dedication Page %%%%%%%%%%%%%%%%%%%%%
\newenvironment{dedication}{%
  \pagestyle{headings}
  \doublespace
  \ifdedicationpage
  \centerline{DEDICATION }
  \fi
  }
  {
  \ifdedicationpage
  \@dedicationtext
    \newpage
  \fi
  }

%%%%%%%%%%%%%%% Acknowledgements Page %%%%%%%%%%%%%%%%%%%%%
\newenvironment{acknowledgments}{%
  \pagestyle{headings}
  \doublespace
  \ifacknowledgmentspage
  \centerline{ACKNOWLEDGMENTS}
    \addcontentsline{toc}{text}{\uppercase{Acknowledgments}}
  \fi
  }
  {
  \ifacknowledgmentspage
  \noindent
   \@acknowledgmenttext
    \newpage
  \fi
  }

%%%%%%%%%%%%%%% Table of Contents Page %%%%%%%%%%%%%%%%%%%%%
\renewenvironment{tableofcontents}{%
  \tableofcontentspage         % Defined Below
  \iftablespage \listoftables \fi  % Defined Below
  \iffigurespage \listoffigures \fi  % Defined Below
  \ifsymbolpage \listofsymbols \input \@symfile \fi
  \addtocontents{toc}{\vskip\parskip\vskip\baselineskip  
    \hbox to \textwidth{CHAPTER \hfill \mbox{}}}
    %\textheight 8.4in     
   \gsutext
  }

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\newcommand{\tableofcontentspage}{
%  \pagestyle{headings}
    \@restonecolfalse
    \if@twocolumn\@restonecoltrue\onecolumn \fi
    \chapterc*{
    \vspace*{-12pt}
       \normalsize\uppercase{Table of Contents} 
     \@mkboth{\@tocheader}{}}
    \vspace*{-12pt}   
    {\iftablespage   \hbox to \textwidth{\mbox{}\hfill Page}
    \else {\iffigurespage \hbox to \textwidth{\mbox{}\hfill Page}
    \else {\ifsymbolpage \hbox to \textwidth{\mbox{}\hfill Page}
    \else \hbox to \textwidth{\@tocheader \hfill Page}
       \fi \relax}
       \fi \relax}
       \fi \relax}
       {\singlespace \@starttoc{toc}}\if@restonecol\twocolumn\fi} 

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\renewcommand{\listoffigures}{
    \@restonecolfalse
    \if@twocolumn\@restonecoltrue\onecolumn \fi
    \chapterc*{LIST OF FIGURES \@mkboth{Figure}{}}
    \vspace*{-12pt} 
    \hbox to \textwidth{Figure \hfill Page}
     \addcontentsline{toc}{text}{LIST OF FIGURES}
    {\singlespace \@starttoc{lof}}\if@restonecol\twocolumn\fi}
  \def\l@figure{\@dottedtocline{0}{1.8em}{1.8em}}

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\renewcommand{\listoftables}{
    \@restonecolfalse
    \if@twocolumn\@restonecoltrue\onecolumn \fi
    \chapterc*{LIST OF TABLES \@mkboth{Table}{}}
    \vspace*{-12pt} 
    \hbox to \textwidth{Table \hfill Page}
     \addcontentsline{toc}{text}{LIST OF TABLES}
    {\singlespace \@starttoc{lot}}\if@restonecol\twocolumn\fi}
    \let\l@table\l@figure

  \def\listofsymbols{\@restonecolfalse
    \if@twocolumn\@restonecoltrue\onecolumn \fi
    \chapterc*{LIST OF SYMBOLS \@mkboth{Symbol}{}}
    \vspace*{10pt}%\mbox{}\\[0.5ex]
     \addcontentsline{toc}{text}{LIST OF SYMBOLS}
    \doublespace\if@restonecol\twocolumn\fi}

